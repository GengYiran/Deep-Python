    "from tqdm import tqdm\n",
    "total = 100000000\n",
    "t = 0\n",
    "with tqdm(total=total) as pbar:\n",
    "\tpbar.set_description('Enumerating envs:')\n",
    "\tfor i in range(total) :\n",
    "\t\tt += i\n",
    "\t\tpbar.update(1)\n",
    "print(t)\n",
    "\n",
    "# 一亿的累和要30秒左右"
